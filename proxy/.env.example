# Keycloak
# The URL to Keycloak, accessible from the proxy container
KEYCLOAK_URL=http://localhost:5050

# Logging
# `debug`, `info`, `warn` or `error`
LOG_LEVEL=debug

# Database
POSTGRES_USER=postgres
POSTGRES_HOST=localhost
POSTGRES_DATABASE_NAME=handler
POSTGRES_PASSWORD=postgres
POSTGRES_PORT=5432

# reCaptcha
# The Google reCaptcha v2 site key generated from their
# admin site
CAPTCHA_SITE_KEY=something